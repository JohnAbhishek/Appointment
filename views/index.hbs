<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <!-- Include Tailwind CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css">
    <link rel="stylesheet" href="/fontawesome/css/all.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <link rel="stylesheet" href="/public/stylesheets/style.css">
    
    <title>Hello, world!</title>
  </head>
  <body style="background-color: rgb(214, 224, 230);">
    <header>
      <nav class="bg-gray-100 p-5">
        <div class="container mx-auto">
          <div class="flex items-center">
            <img width="80px" height="60px" class="rounded-full" src="data:image/jpeg;base64,"/> 
            <a class="text-2xl font-semibold ml-5" href="#">Appointments</a>
            <button class="lg:hidden" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
          </div>
        </div>
      </nav>
    </header>
    
    <section>
      <div class="container mx-auto mt-20">
        <div class="flex flex-wrap">
          {{#each appointments}}
            <div class="w-1/4 p-4">
              <div class="border border-black border-opacity-25 rounded p-4 snake-border animate-snake">
                <h5 class="font-bold text-xl">Event {{this.Appointmentcount}}</h5>
                <p class="mb-2"><b>Start Time:</b> {{this.Time}}</p>
                <p class="mb-2"><b>End Time:</b> {{this.EndTime}}</p>
                <button class="bg-green-500 hover:bg-green-600 text-white py-2 px-4 rounded" onclick="window.location.href = '/edit/{{this.Appointmentcount}}'">Edit</button>
                <button class="bg-red-500 hover:bg-red-600 text-white py-2 px-4 rounded" onclick="window.location.href = '/remove-appoint/{{this.Appointmentcount}}'">Remove</button>
              </div>
            </div>
          {{/each}}
              <div class="flex items-center justify-center">
          <button id="plusIcon" class="bg-yellow-500 hover:bg-yellow-600 text-white py-2 px-4 rounded-full h-24 w-24"
           onclick="window.location.href = '/add'"><i id="plusIcon"  class="fas fa-plus text-6xl"></i>
</button>
        </div>
        </div>
      </div>
    </section>
    <style>

#plusIcon:hover {
  color: rgb(140, 135, 219); /* Set the color of the icon when hovered */
}
    </style>
    <style>
    @keyframes snake {
        0% {
            border-bottom-color: white; 
        }
        25% {
            border-left-color: white;
        }
        50% {
            border-top-color: white;
        }
        75% {
            border-right-color: white;
        }
        
    }

    /* Apply the animation to the border */
    .animate-snake {
        animation: snake 4s ; /* Adjust the duration and timing as needed */
    }
</style>

    <script src="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
  </body>
</html>
